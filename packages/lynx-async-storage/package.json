{
  "name": "@kafitra/lynx-async-storage",
  "version": "0.1.0",
  "description": "Async key-value storage for the Lynx runtime â€” mirrors React Native AsyncStorage",
  "keywords": [
    "lynx",
    "async-storage",
    "key-value",
    "storage",
    "react-native"
  ],
  "author": "Kafitra",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kafitramarna/kafitra-lynx-native.git",
    "directory": "packages/lynx-async-storage"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
